@model QLTYT.Models.LoaiPhong
@{
    ViewBag.Title = "ThemMoiLoaiPhong";
    Layout = null;
}
<html>
<head>
    <meta charset="utf-8">
    <title>Thêm loại phòng</title>
    <link href="~/css/asset/style1.css" rel="stylesheet" />
</head>
<body>
    <div class="wrapper">
        <div class="title">
            Thêm loại phòng
        </div>
        <form method="post" action="ThemMoiLoaiPhong">
            <div class="field">
                <input type="text" name="TenLoaiPhong">
                <label>Tên loại phòng</label>
            </div>

            <div class="field">
                <input type="submit" value="Thêm">
            </div>
            <div class="signup-link">
                <a href="/LoaiPhong/LoaiPhong">Quay lại</a>
            </div>
        </form>
    </div>
</body>
</html>

