@model QLTYT.Models.LoaiPhong
@{
    ViewBag.Title = "SuaDoiLoaiPhong";
    Layout = null;
}
<html>
<head>
    <meta charset="utf-8">
    <title>Sửa đổi loại phòng</title>
    <link href="~/css/asset/style1.css" rel="stylesheet" />
</head>
<body>
    <div class="wrapper">
        <div class="title">
            Sửa đổi loại phòng
        </div>
        <form method="post" action="SuaDoiLoaiPhong" enctype="multipart/form-data">
            <div class="field">
                <input type="text" name="IdLoaiPhong" value="@Model.IdLoaiPhong" readonly>

            </div>
            <div class="field">
                <input type="text" name="TenLoaiPhong" value="@Model.TenLoaiPhong">
                <label>Tên loại phòng</label>
            </div>

            <div class="field">
                <input type="submit" value="Cập nhật">
            </div>
            <div class="signup-link">
                <a href="/LoaiPhong/LoaiPhong">Quay lại</a>
            </div>
        </form>
    </div>
</body>
</html>


